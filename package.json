{
  "name": "gliwicka-111-website",
  "version": "1.0.0",
  "private": true,
  "description": "Professional business center website with comprehensive form system and analytics",
  "keywords": [
    "nextjs",
    "typescript",
    "business",
    "forms",
    "analytics"
  ],
  "license": "MIT",
  "author": "Gliwicka 111 Team",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:bundle": "npx @next/bundle-analyzer",
    "build": "next build",
    "clean": "rimraf .next out dist coverage test-results",
    "db:migrate": "node scripts/migrate-database.js",
    "db:reset": "node scripts/reset-database.js",
    "db:seed": "node scripts/seed-database.js",
    "db:seed:test": "node scripts/seed-test-data.js",
    "deploy:production": "node scripts/deploy-production.js",
    "deploy:staging": "node scripts/deploy-staging.js",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:docs": "typedoc --out docs src",
    "health-check": "node scripts/health-check.js",
    "postinstall": "husky install",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "start": "next start",
    "test": "vitest",
    "test:a11y": "node scripts/run-accessibility-tests.js",
    "test:build": "node scripts/test-build.js",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:performance": "node scripts/run-benchmarks.js",
    "test:smoke": "node scripts/smoke-tests.js",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --coverage",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test:unit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@edge-runtime/vm": "latest",
    "@hookform/resolvers": "latest",
    "@jest/globals": "latest",
    "@next/bundle-analyzer": "^14.0.4",
    "@radix-ui/react-accordion": "latest",
    "@radix-ui/react-alert-dialog": "latest",
    "@radix-ui/react-aspect-ratio": "latest",
    "@radix-ui/react-avatar": "latest",
    "@radix-ui/react-checkbox": "latest",
    "@radix-ui/react-collapsible": "latest",
    "@radix-ui/react-context-menu": "latest",
    "@radix-ui/react-dialog": "latest",
    "@radix-ui/react-dropdown-menu": "latest",
    "@radix-ui/react-hover-card": "latest",
    "@radix-ui/react-label": "latest",
    "@radix-ui/react-menubar": "latest",
    "@radix-ui/react-navigation-menu": "latest",
    "@radix-ui/react-popover": "latest",
    "@radix-ui/react-progress": "latest",
    "@radix-ui/react-radio-group": "latest",
    "@radix-ui/react-scroll-area": "latest",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-separator": "latest",
    "@radix-ui/react-slider": "latest",
    "@radix-ui/react-slot": "latest",
    "@radix-ui/react-switch": "latest",
    "@radix-ui/react-tabs": "latest",
    "@radix-ui/react-toast": "latest",
    "@radix-ui/react-toggle": "latest",
    "@radix-ui/react-toggle-group": "latest",
    "@radix-ui/react-tooltip": "latest",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/dom": "latest",
    "@types/debug": "latest",
    "@vitest/browser": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "latest",
    "crypto": "latest",
    "date-fns": "^2.30.0",
    "embla-carousel-react": "latest",
    "expect": "latest",
    "fs": "latest",
    "input-otp": "latest",
    "jest": "latest",
    "lucide-react": "^0.454.0",
    "mysql2": "latest",
    "next": "^14.2.31",
    "next-themes": "latest",
    "node-notifier": "latest",
    "path": "latest",
    "react": "^18",
    "react-day-picker": "latest",
    "react-dom": "^18",
    "react-hook-form": "latest",
    "react-resizable-panels": "latest",
    "recharts": "latest",
    "sonner": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "latest",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/node": "^22",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.3",
    "cross-env": "^7.0.3",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "jest-axe": "^8.0.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.5",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.17",
    "typedoc": "^0.25.4",
    "typescript": "^5",
    "vite": "^7.0.6",
    "vitest": "^3.2.4",
    "wait-on": "^7.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
