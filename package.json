{
  "name": "gliwicka-111-website",
  "version": "1.0.0",
  "private": true,
  "description": "Professional business center website with comprehensive form system and analytics",
  "keywords": [
    "nextjs",
    "typescript",
    "business",
    "forms",
    "analytics"
  ],
  "license": "MIT",
  "author": "Gliwicka 111 Team",
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:bundle": "npx @next/bundle-analyzer",
    "build": "next build",
    "clean": "rimraf .next out dist coverage test-results",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate:docs": "typedoc --out docs src",
    "postinstall": "husky install",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "migrate": "ts-node scripts/migrate.ts",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "start": "next start",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "npm run build && playwright test -c e2e/playwright.config.ts",
    "test:integration": "vitest run --config vitest.integration.config.mts",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run --coverage",
    "test:watch": "vitest",
    "type-check": "tsc --noEmit",
    "retry:failed-emails": "ts-node scripts/retry-failed-emails.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm run test:unit"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
