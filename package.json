    "@radix-ui/react-toggle-group": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.7",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^2.30.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.454.0",
    "next": "^14.2.31",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.5",
    "pg": "^8.16.3",
    "react": "^18.3.1",
